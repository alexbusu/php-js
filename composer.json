{
  "name": "alexbusu/php-js",
  "description": "Micro-framework which translates PHP-defined actions in browser using jQuery",
  "version": "3.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandru Busuioc",
      "email": "busuioc.alexandru@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.0.0",
    "psalm/phar": ">=5.14.0",
    "rector/rector": ">=0.18.0"
  },
  "autoload": {
    "psr-4": {
      "Alexbusu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alexbusu\\Tests\\Unit\\": "tests/unit"
    }
  },
  "config": {
    "allow-plugins": {
      "ocramius/package-versions": true
    }
  }
}
